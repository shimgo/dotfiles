[user]
	name = shimgo
	email = fen.mex.cyc@gmail.com
[credential]
	helper = osxkeychain
[pull]
	rebase = false
[ghq]
    root = ~/go/src
    root = ~/ghq
[alias]
	see = browse
	st = status
	ch = checkout
	w = worktree
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[mergetool "vimdiff"]
	path = nvim
[difftool]
	prompt = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
